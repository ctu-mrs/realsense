<launch>
  <arg name="uav_name"           default="$(optenv UAV_NAME uav)"/>
  <arg name="fcu_frame" default="fcu_$(arg uav_name)"/>
  <arg name="sensor_frame" default="camera_color_optical_frame"/>

  <node pkg="tf" type="static_transform_publisher" name="rsd435front2fcu" args="0.27 0.0 -0.05 -1.57 0 -1.57 $(arg fcu_frame) $(arg sensor_frame) 100" />
</launch>
