<launch>

  <arg name="uav_name" default="$(optenv UAV_NAME uav)" />
  <arg name="trajectory" default="" />

  <group ns="$(arg uav_name)">

    <node name="trajectory_loader" pkg="mrs_multimaster" type="multimaster_trajectories_loader" args="--start" output="screen" >

      <rosparam file="$(find lidar_localization)/config/multimaster_trajectory_$(arg trajectory).yaml" />
      <rosparam file="$(find trajectory_handler)/config/params.yaml" />
      <param name="current_working_directory" type="string" value="$(find lidar_localization)" />	

    </node>

  </group>

</launch>
